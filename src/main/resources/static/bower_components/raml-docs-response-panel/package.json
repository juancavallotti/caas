{
  "name": "raml-docs-response-panel",
  "version": "1.1.12",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "Element that displays response documentation for RAML",
  "devDependencies": {
    "polymer-cli": "^1.2.0",
    "bower": "^1.8.0"
  },
  "private": false,
  "main": [
    "raml-docs-response-panel.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint raml-docs-response-panel.html raml-docs-response-status-selector.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"raml-docs-response-panel:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/raml-docs-response-panel.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/raml-docs-response-panel/issues",
    "email": "arc@mulesoft.com"
  }
}