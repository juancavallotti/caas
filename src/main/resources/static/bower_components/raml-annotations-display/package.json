{
  "name": "raml-annotations-display",
  "version": "0.1.5",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A display element for RAML annotations",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "raml-annotations-display.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint raml-annotations-display.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"raml-annotations-display:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/raml-annotations-display.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/raml-annotations-display/issues",
    "email": "arc@mulesoft.com"
  }
}