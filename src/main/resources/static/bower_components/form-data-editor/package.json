{
  "name": "form-data-editor",
  "version": "0.2.2",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "An element to edit form data (x-www-form-urlencoded)",
  "devDependencies": {
    "polymer-cli": "^1.2.0"
  },
  "main": [
    "form-data-editor.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint form-data-editor.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"form-data-editor:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/form-data-editor.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/form-data-editor/issues",
