{
  "name": "files-payload-editor",
  "version": "0.1.4",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A request body editor to add files as a payload.",
  "devDependencies": {
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "files-payload-editor.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint files-payload-editor.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"files-payload-editor:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/files-payload-editor.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/files-payload-editor/issues",
    "email": "arc@mulesoft.com"
  }
}